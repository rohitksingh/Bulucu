<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freewifi.rohksin.freewifi" >

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".TestActivity">

        </activity>
        <activity
            android:name=".Activities.WifiDetailActivity"
            android:label="Detail">
        </activity>
        <activity android:name=".Activities.WifiPagerActivity">

        </activity>
        <activity android:name=".Activities.ScanSurrounding"
            >

        </activity>
        <activity android:name=".Activities.WifiMainActiity"
            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.OpenWifiActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <service android:name=".Services.WifiSignalService"/>
        <service android:name=".Services.ScanWifiService"/>
    </application>

</manifest>
